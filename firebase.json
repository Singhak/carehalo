{
  "functions": [
    {
      "source": "apps/functions",
      "codebase": "default"
    }
  ],
  "hosting": {
    "public": "apps/web-app/dist/web-app",
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}